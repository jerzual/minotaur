{
  "name": "minotaur",
  "version": "1.0.0",
  "description": "an attempt at making a browser FPS with THREE.js",
  "main": "src/scripts/minotaur.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress",
    "start": "webpack-dev-server --progress --hot --inline",
    "html": "pug -o www/ src/index.pug",
    "css": "node-sass -o www/styles/ ./src/styles/index.scss",
    "lint": "eslint --ext .js,.jsx src test/unit",
    "test": "jest"
  },
  "author": "jibhaine",
  "license": "MIT",
  "repository": "https://github.com/jerzual/minotaur.git",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "canvas": "^1.6.7",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.28.5",
    "del": "^2.2.0",
    "enzyme": "^3.2.0",
    "eslint": "^4.12.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-react": "^7.5.1",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "npm-run-all": "^1.7.0",
    "offline-plugin": "^4.8.3",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-loader": "^2.0.6",
    "preact-compat": "^3.17.0",
    "preact-compat-enzyme": "^0.2.5",
    "preact-jsx-chai": "^2.2.1",
    "preact-render-to-string": "^3.7.0",
    "preact-test-utils": "^0.1.3",
    "proptypes": "^1.1.0",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "dat-gui": "^0.5.0",
    "easystarjs": "^0.3.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.1.0",
    "pouchdb": "^6.3.4",
    "preact": "^8.2.1",
    "preact-redux": "^2.0.3",
    "preact-router": "^2.5.5",
    "preact-transition-group": "^1.1.1",
    "redux": "^3.7.2",
    "redux-pouchdb": "^0.1.1",
    "rot-js": "^0.6.2",
    "three": "^0.73.2"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/mocks/styleMock.js",
      "^react-dom/server$": "<rootDir>/node_modules/preact-render-to-string/dist/index.js",
      "^react-addons-test-utils$": "<rootDir>/node_modules/preact-test-utils/lib/index.js",
      "^react$": "<rootDir>/node_modules/preact-compat-enzyme/lib/index.js",
      "^react-dom$": "<rootDir>/node_modules/preact-compat-enzyme/lib/index.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
